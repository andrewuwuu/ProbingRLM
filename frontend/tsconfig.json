{
  "compilerOptions": {
    "target": "ES2021",
    "lib": [
      "ES2021",
      "DOM"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "strict": true,
    "types": [
      "vite/client"
    ],
    "noEmit": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src"
  ]
}